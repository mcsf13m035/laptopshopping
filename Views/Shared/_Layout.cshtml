    <script type="text/javascript">
        var flashvars = {};
        flashvars.cssSource = "~/Content/piecemaker.css";
        flashvars.xmlSource = "~/Content/piecemaker.xml";
        var params = {};
        params.play = "false";
        params.menu = "false";
        params.scale = "showall";
        params.wmode = "transparent";
        params.allowfullscreen = "true";
        params.allowscriptaccess = "sameDomain";
        params.allownetworking = "all";
        swfobject.embedSWF('~/Scripts/piecemaker.swf', 'piecemaker', '960', '430', '10', null, flashvars, params, null);
    </script>
